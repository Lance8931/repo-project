  </select>
  <select id="getKnowById" resultType="java.lang.Integer" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List"/>
    from knowledge_type where id=#{id,jdbcType=INTEGER}
  </select>
  <select id="selectNewName" parameterType="com.siping.system.bean.KnowledgeType" resultType="java.lang.Integer">
    select count(t.name) from knowledge_type t where t.name = #{name,jdbcType=VARCHAR}
  </select>
</mapper>