    insert into knowledge_type (id, name, knowledge_type, 
      add_time, add_by, update_time, 
      update_by)
    values (#{id,jdbcType=INTEGER}, #{name,jdbcType=VARCHAR}, #{knowledgeType,jdbcType=INTEGER}, 
      STR_TO_DATE(#{addTime,jdbcType=TIMESTAMP},'%Y-%m-%d %H:%i:%s'), #{addBy,jdbcType=INTEGER},  STR_TO_DATE(#{updateTime,jdbcType=TIMESTAMP},'%Y-%m-%d %H:%i:%s'), 
      #{updateBy,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="com.siping.system.bean.KnowledgeType" >
    insert into knowledge_type
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        id,
      </if>
      <if test="name != null" >
        name,
      </if>
      <if test="knowledgeType != null" >
        knowledge_type,
      </if>
      <if test="addTime != null" >
        add_time,
      </if>
      <if test="addBy != null" >
        add_by,
      </if>
      <if test="updateTime != null" >
        update_time,
      </if>
      <if test="updateBy != null" >
        update_by,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        #{id,jdbcType=INTEGER},
      </if>
      <if test="name != null" >
        #{name,jdbcType=VARCHAR},
      </if>
      <if test="knowledgeType != null" >
        #{knowledgeType,jdbcType=INTEGER},
      </if>
      <if test="addTime != null" >
        STR_TO_DATE(#{addTime,jdbcType=TIMESTAMP},'%Y-%m-%d %H:%i:%s'),
      </if>
      <if test="addBy != null" >
        #{addBy,jdbcType=INTEGER},
      </if>
      <if test="updateTime != null" >
       STR_TO_DATE(#{updateTime,jdbcType=TIMESTAMP},'%Y-%m-%d %H:%i:%s'),
      </if>
      <if test="updateBy != null" >
        #{updateBy,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
  <update id="updateByPrimaryKeySelective" parameterType="com.siping.system.bean.KnowledgeType" >
    update knowledge_type
    <set >
      <if test="name != null" >
        name = #{name,jdbcType=VARCHAR},
      </if>
      <if test="knowledgeType != null" >
        knowledge_type = #{knowledgeType,jdbcType=INTEGER},
      </if>
      <if test="addTime != null" >
        add_time = STR_TO_DATE(#{addTime,jdbcType=TIMESTAMP},'%Y-%m-%d %H:%i:%s'),
      </if>
      <if test="addBy != null" >
        add_by = #{addBy,jdbcType=INTEGER},
      </if>
      <if test="updateTime != null" >
        update_time = STR_TO_DATE(#{updateTime,jdbcType=TIMESTAMP},'%Y-%m-%d %H:%i:%s'),
