  </update>
  <update id="updateByPrimaryKey" parameterType="com.siping.system.bean.KnowledgeType" >
    update knowledge_type
    set name = #{name,jdbcType=VARCHAR},
      knowledge_type = #{knowledgeType,jdbcType=INTEGER},
      add_time = STR_TO_DATE(#{addTime,jdbcType=TIMESTAMP},'%Y-%m-%d %H:%i:%s'),
      add_by = #{addBy,jdbcType=INTEGER},
      update_time =  STR_TO_DATE(#{updateTime,jdbcType=TIMESTAMP},'%Y-%m-%d %H:%i:%s'),
      update_by = #{updateBy,jdbcType=INTEGER}
    where id = #{id,jdbcType=INTEGER}
  </update>
  <sql id="conditions" >
    <trim prefix="where" prefixOverrides="and|or" >
       1 = 1
      <if test="id != null" >
        and t.id = #{id}
      </if>
      <if test="name != null" >
        and t.name = #{name}
      </if>
      <if test="knowledgeType != null" >
        and t.knowledge_type = #{knowledgeType}
      </if>
      <if test="addTime != null" >
        and t.add_time = #{addTime}
      </if>
      <if test="addBy != null" >
        and t.add_by = #{addBy}
      </if>
      <if test="updateTime != null" >
        and t.update_time = #{updateTime}
      </if>
      <if test="updateBy != null" >
        and t.update_by = #{updateBy}
      </if>
    </trim>
  </sql>
  <sql id="sets" >
    <trim prefix="SET" suffixOverrides="," >
      <if test="name != null" >
        t.name = #{name}
      </if>
      <if test="knowledge_type != null" >
        t.knowledge_type = #{knowledgeType}
      </if>
      <if test="add_time != null" >
        t.add_time = #{addTime}
      </if>
      <if test="add_by != null" >
        t.add_by = #{addBy}
      </if>
      <if test="update_time != null" >
        t.update_time = #{updateTime}
      </if>
      <if test="update_by != null" >
        t.update_by = #{updateBy}
      </if>
    </trim>
  </sql>
  <select id="getAll" resultMap="BaseResultMap" >
    select * from knowledge_type
  </select>
  <delete id="deleteByIds" >
     delete from knowledge_type where id in 
    <foreach collection="list" item="item" index="index" separator="," open="(" close=")" >
      #{item}
    </foreach>
  </delete>
  <select id="get" parameterType="java.util.Map" resultMap="BaseResultMap" >
    select *  from knowledge_type t
