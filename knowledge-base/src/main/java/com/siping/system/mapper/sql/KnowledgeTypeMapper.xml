  </select>
  <select id="getCount" parameterType="java.util.Map" resultType="java.lang.Long" >
     select count(*) from knowledge_type t
    <include refid="conditions"/>
  </select>
  <select id="getByPage" parameterType="com.siping.web.bean.PageRequest" resultMap="BaseResultMap" >
    select 
     <include refid="Base_Column_List" />
     from knowledge_type t
    <if test="startNo != null" >
      LIMIT #{startNo},#{pageSize}
    </if>
  </select>
  <select id="getTotalKnow" resultType="java.lang.Integer">
    select count(k.id) from knowledge_type k
  </select>
  <select id="getProblem" resultType="java.lang.Integer">
