<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.phoneerp.dao.AllotDetailMapper" >
  <resultMap id="BaseResultMap" type="com.phoneerp.bean.AllotDetail" >
    <result column="id" property="id" jdbcType="INTEGER" />
    <result column="phone_id" property="phoneId" jdbcType="INTEGER" />
  </resultMap>
  <insert id="insert" parameterType="com.phoneerp.bean.AllotDetail" >
    insert into allot_detail (id, phone_id)
    values (#{id,jdbcType=INTEGER}, #{phoneId,jdbcType=INTEGER})
  </insert>
  <insert id="insertSelective" parameterType="com.phoneerp.bean.AllotDetail" >
    insert into allot_detail
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        id,
      </if>
      <if test="phoneId != null" >
        phone_id,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="id != null" >
        #{id,jdbcType=INTEGER},
      </if>
      <if test="phoneId != null" >
        #{phoneId,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
</mapper>